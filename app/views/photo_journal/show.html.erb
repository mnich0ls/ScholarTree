<% content_for :head do %>
<% end %>
<script type="text/template" id="entry-template">
    <div class="entry-title-container">
        <h5><%%= title %></h5>
    </div>
    <div class="entry-snippet-container"><%%= snippet %></div>
</script>

<script type="text/template" id="photo-template">
    <img src="<%%= url %>" alt="<%%= description %>" height="<%%= height %>" width="<%%= width %>">
</script>

<script type="text/javascript">
    $('#journal-container').ready(function(){
        window.journalContainer = new JournalContainer();
        window.journalContainer.searchQuery = "April 2013";
        window.journalContainer.render();
    });
</script>
<div id="journal-container" class="search-context" search-url="/photo_journal/show"></div>

<div id="journal-item-modal" class="modal hide fade">
  <div class="modal-header">
   <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
   <h3>Testing Header</h3>
  </div>
  <div class="modal-body">
    <p>Testing the body</p>
  </div>
  <div class="modal-footer">
    <a href="#" class="btn" data-dismiss="modal">Close</a>
    <a href="#" class="btn btn-primary" data-dismiss="modal">Edit</a>
  </div>
</div>
